<Project Sdk="MSBuild.Sdk.Extras/3.0.23">
  <PropertyGroup>
    <ToolkitComponentName>AccentColorExtractor</ToolkitComponentName>
  </PropertyGroup>

  <!-- Sets this up as a toolkit component's sample project -->
  <Import Project="$(ToolingDirectory)\ToolkitComponent.SampleProject.props" />
  <ItemGroup>
    <UpToDateCheckInput Remove="AccentColorExtractorSample.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="AccentColorExtractorSample.xaml.cs">
      <DependentUpon>AccentColorExtractorSample.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>
